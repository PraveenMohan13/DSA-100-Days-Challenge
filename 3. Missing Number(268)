https://leetcode.com/problems/missing-number/

class Solution {
    public int missingNumber(int[] nums) {
        int n=nums.length;
        Arrays.sort(nums);
        int j;
        for(int i=0;i<n;i++)
        {
            for(j=0;j<n;j++)
            {
                if(i==nums[i])
                break;
            }
            if(j==n)
            return i;
        }
    return n;     
    }
}
