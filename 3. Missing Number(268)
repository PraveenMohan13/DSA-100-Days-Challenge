https://leetcode.com/problems/missing-number/


class Solution {
    public int missingNumber(int[] nums) {
        int n=nums.length;
        Arrays.sort(nums);
        int j;
        for(int i=0;i<n;i++)
        {
            if(i!=nums[i])
            return i;     
        }
    return n;     
    }
}
